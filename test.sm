@import println from "std.io"

func main() do
    for
        var a: u64 = 0,
        var b: u64 = 1,
        var count: u64 = 0;
        while count < 75;
        step
            println("tick"),
            var temp: u64 = a,
            a = b,
            b = temp + b,
            count = count + 1
    do
        println("Fibonacci:", a)
    end
end